{
  "name": "memory",
  "version": "1.0.0",
  "description": "A mini project to study react SSR.",
  "main": "main.js",
  "scripts": {
    "build:client": "webpack --config ./build/webpack-client --progress --colors",
    "build:ssr": "webpack --config ./build/webpack-server --progress --colors",
    "build:client:watch": "yarn run build:client --watch",
    "build:ssr:watch": "yarn run build:ssr --watch",
    "build:client:stats": "webpack --config ./build/webpack-client --progress --colors --profile --json > ./dist/client/stats.json",
    "build:ssr:stats": "webpack --config ./build/webpack-server --progress --colors --profile --json > ./dist/server/stats.json",
    "bundle-analyzer:client": "webpack-bundle-analyzer ./dist/client/stats.json",
    "bundle-analyzer:ssr": "webpack-bundle-analyzer ./dist/server/stats.json",
    "serve": "cd dist/server && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xbini/memory.git"
  },
  "keywords": [
    "react",
    "ssr",
    "server-side-render",
    "webpack"
  ],
  "author": "xluobin@163.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xbini/memory/issues"
  },
  "homepage": "https://github.com/xbini/memory#readme",
  "dependencies": {
    "antd-mobile": "^2.2.6",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "autoprefixer": "^9.4.2",
    "css-loader": "^2.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.6",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.5"
  }
}
